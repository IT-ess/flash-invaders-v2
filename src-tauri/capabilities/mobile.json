{
	"$schema": "../gen/schemas/mobile-schema.json",
	"identifier": "mobile",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"core:path:default",
		"core:event:default",
		"core:window:default",
		"core:app:default",
		"core:image:default",
		"core:resources:default",
		"core:menu:default",
		"core:tray:default",
		"shell:allow-open"
	],
	"platforms": ["iOS", "android"]
}
